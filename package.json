{
	"name": "bookit-mern",
	"version": "1.0.0",
	"description": "BookIt - MERN Stack Application",
	"main": "server/server.js",
	"scripts": {
		"start": "concurrently \"npm run server\" \"npm run client\"",
		"server": "nodemon server/server.js",
		"client": "cd client && npm start",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"install-all": "npm install && cd client && npm install",
		"build": "cd client && npm run build",
		"server-only": "node server/server.js",
		"client-only": "cd client && npm start"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.2",
		"connect-flash": "^0.1.1",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.3.1",
		"ejs": "^3.1.9",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"lodash": "^4.17.21",
		"method-override": "^3.0.0",
		"mongodb": "^3.7.4",
		"mongoose": "^5.13.20",
		"morgan": "^1.10.0",
		"mpath": "^0.9.0",
		"multer": "^1.4.5-lts.1",
		"node-fetch": "^2.7.0",
		"passport": "^0.6.0",
		"passport-facebook": "^3.0.0",
		"passport-goodreads": "^1.0.0",
		"passport-google-oauth": "^2.0.0",
		"passport-local": "^1.0.0",
		"passport-twitter": "^1.0.4",
		"xml-js": "^1.6.11",
		"xml2js": "^0.6.2"
	},
	"devDependencies": {
		"concurrently": "^8.2.2",
		"cors": "^2.8.5",
		"nodemon": "^3.1.10"
	}
}
